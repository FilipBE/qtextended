<?xml version="1.0"?>
<page xmlns="http://www.trolltech.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.trolltech.com themedview.xsd" name="secondarytitle" base="themes/home_wvga" rect="0,0,0x17">
    <image rect="0,0,0x17" src="titlebar-gradient" tile="yes" scale="vertical" alpha="150" color="#000000"/>
    <group>
        <level name="signal" rect="2,1,8x15" src="signal.svg" count="6" min="0" max="100" scale="yes" active="expr:@/Hardware/Accessories/QSignalSource/DefaultSignal/SignalStrength">
            expr:@/Hardware/Accessories/QSignalSource/DefaultSignal/SignalStrength
        </level>
        <level name="battery" rect="-10,1,8x15" src="battery.svg" count="6" min="0" max="100" scale="yes" delay="1000" loop="0" play="expr:@/Hardware/Accessories/QPowerSource/DefaultBattery/Charging == true">
            expr:@/Hardware/Accessories/QPowerSource/DefaultBattery/Charge
        </level>
    </group>
</page>
